{
  "name": "pride",
  "version": "0.0.1",
  "main": "pride.min.js",
  "scripts": {
    "build": "npm run esbuild:regular && npm run esbuild:minify && npm run esbuild:spec",
    "esbuild": "npx esbuild src/${SOURCE_FILE-index}.js --bundle --format=cjs $*",
    "esbuild:minify": "npm run esbuild -- --outfile=pride.min.js --minify",
    "esbuild:regular": "npm run esbuild -- --outfile=pride.js",
    "esbuild:spec": "SOURCE_FILE=spec npm run esbuild -- --outfile=spec/pride.spec.js",
    "eslint": "npx eslint $* source/**/*.js spec/**/*.js",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "dependencies": {
    "esbuild": "^0.19.4",
    "pegjs": "^0.10.0",
    "reqwest": "^2.0.5",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1"
  }
}
