{
  "name": "pride",
  "version": "1.0.0",
  "main": "pride.min.js",
  "scripts": {
    "build": "npm run eslint && npm run esbuild:minify && npm run esbuild:regular",
    "esbuild": "npx esbuild src/index.js --bundle --format=cjs $*",
    "esbuild:minify": "npm run esbuild -- --outfile=pride.min.js --minify",
    "esbuild:regular": "npm run esbuild -- --outfile=pride.js",
    "eslint": "npx eslint $* './src/**/*.js' './spec/**/*.js'",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "npx mocha --recursive './spec/**/*.spec.js'"
  },
  "dependencies": {
    "esbuild": "^0.19.4",
    "pegjs": "^0.10.0",
    "reqwest": "^2.0.5",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0"
  }
}
