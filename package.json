{
  "name": "pride",
  "version": "0.0.1",
  "main": "pride.js",
  "scripts": {
    "build": "gulp",
    "esbuild": "npm run esbuild:regular && npm run esbuild:minify && npm run esbuild:exec",
    "esbuild:exec": "BUNDLE=exec node concat.js && npx esbuild output-exec.js --bundle --format=cjs --outfile=pride.execjs.js",
    "esbuild:minify": "node concat.js && npx esbuild output.js --bundle --format=cjs --outfile=pride.min.js --minify --analyze",
    "esbuild:regular": "node concat.js && npx esbuild output.js --bundle --format=cjs --outfile=pride.js",
    "gulp": "gulp"
  },
  "dependencies": {
    "gulp-clean": "^0.4.0",
    "gulp-pegjs": "^0.2.0",
    "pegjs": "^0.10.0",
    "reqwest": "^2.0.5",
    "underscore": "^1.13.6",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "babel-preset-env": "^1.7.0",
    "esbuild": "^0.19.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-jshint": "^2.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-uglify": "^3.0.2",
    "jshint": "^2.13.6"
  },
  "overrides": {
    "chokidar": "^3.5.3",
    "glob-parent": "^6.0.2"
  }
}
